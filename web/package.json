    "eslint-watch": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^0.20.2",
    "vite-plugin-svgr": "^4.2.0"
  }
