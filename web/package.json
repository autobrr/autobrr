    "http-proxy-middleware": "^2.0.6",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
